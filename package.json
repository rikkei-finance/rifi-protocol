{
  "name": "rifi-protocol",
  "version": "0.2.1",
  "description": "Rikkei Finance Market",
  "main": "index.js",
  "scripts": {
    "compile": "./script/compile",
    "console": "if node -v | grep -E \"v(12|13)\" > /dev/null; then flags=\"-n --experimental-repl-await\"; fi; npx $flags saddle console",
    "coverage": "./script/coverage",
    "deploy": "./scenario/script/repl -s ./script/scen/deploy.scen",
    "lint": "./script/lint",
    "repl": "./scenario/script/repl",
    "profile": "yarn test tests/gasProfiler.js",
    "report": "npx run ./script/saddle/gasReport.js",
    "test": "./script/test",
    "test:prepare": "NO_RUN=true ./script/test",
    "checksize": "npx run ./script/saddle/cointrollerSize.js",
    "migrate:bsc_testnet": "npx hardhat run --network bsc_testnet ./deploy/lending.js",
    "migrate:moonbase": "npx hardhat run --network moonbase ./deploy/lending.js",
    "migrate:localhost": "npx hardhat run --network localhost ./deploy/lending.js",
    "migrate:goerli": "npx hardhat run --network goerli ./deploy/lending.js",  
    "upgrade:bsc_testnet": "npx hardhat run --network bsc_testnet ./deploy/upgrade.js",
    "upgrade:moonbase": "npx hardhat run --network moonbase ./deploy/upgrade.js"
  },
  "repository": "git@github.com:rifi-finance/rifi-protocol.git",
  "author": "Rikkei Finance, Inc",
  "license": "UNLICENSED",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "^2.5.1",
    "@truffle/hdwallet-provider": "1.2.3",
    "abi-decoder": "^2.3.0",
    "bignumber.js": "9.0.0",
    "dotenv": "^8.2.0",
    "ethereumjs-tx": "^2.1.2",
    "fs": "^0.0.1-security",
    "hardhat": "^2.6.2",
    "jest-diff": "^26.4.2",
    "jest-junit": "^11.1.0",
    "minimist": "^1.2.5",
    "prettier": "^2.0.5",
    "solidity-coverage": "0.7.0",
    "solium": "^1.2.5",
    "solparse": "^2.2.8",
    "truffle": "5.1.31",
    "truffle-flattener": "^1.4.4",
    "truffle-plugin-verify": "^0.5.13",
    "web3": "^1.3.0",
    "web3-eth-abi": "^1.3.0"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "openzeppelin-test-helpers": "^0.5.1"
  }
}
